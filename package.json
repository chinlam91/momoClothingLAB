{
  "name": "osint",
  "version": "1.0.0",
  "description": "OSINT front-end",
  "main": "server.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-client": "grunt prod && rsync -av ./assets ./build --exclude less && cd ./build/assets/js && mv app.min.js app.js && mv vendor.min.js vendor.js",
    "build-server": "cp -R ./locales ./build",
    "build": "npm run clean && npm run build-server && npm run build-client && cd build && npm install --production --no-bin-links",
    "build-dev": "webpack",
    "build-prod": "NODE_ENV=production webpack -p --progress",
    "lint": "./node_modules/eslint/bin/eslint.js \"src/**/*.js\" > lint.log",
    "start": "babel-node server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.1",
    "classnames": "^2.2.5",
    "errorhandler": "^1.4.3",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "fs-extra": "^0.30.0",
    "lodash": "^4.13.1",
    "method-override": "^2.3.6",
    "moment": "^2.13.0",
    "morgan": "^1.7.0",
    "react": "^15.1.0",
    "react-addons-css-transition-group": "^15.1.0",
    "react-addons-pure-render-mixin": "^15.1.0",
    "react-addons-update": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-draggable": "^2.1.1",
    "serve-favicon": "^2.3.0",
    "underscore.string": "^3.3.4"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.7",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "eslint": "^3.5.0",
    "eslint-plugin-react": "^5.1.1",
    "grunt": "^1.0.1",
    "grunt-browserify": "^5.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-less": "^1.3.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-env": "^0.4.4",
    "grunt-eslint": "^18.1.0",
    "webpack": "^1.13.1"
  }
}
